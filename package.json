        "react-intersection-observer": "^9.5.3",
        "react-pdf": "6.2.2",
        "react-router-dom": "^6.21.1",
        "tailwind-merge": "^2.5.4"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/plugin-transform-runtime": "^7.25.9",
