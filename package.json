{
  "name": "portfolio-redesign",
  "private": true,
  "version": "0.25.50",
  "description": "Ralph Bernard Serrano's Portfolio Website - A modern, responsive portfolio built with React and TypeScript",
  "type": "module",
  "homepage": "https://va-ralphbserrano.github.io/va-rb-portfolio/",
  "repository": {
    "type": "git",
    "url": "https://github.com/va-ralphbserrano/va-rb-portfolio.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx,js,jsx --fix",
    "typecheck": "tsc --noEmit",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write .",
    "clean": "rimraf dist node_modules",
    "analyze": "vite build && rollup-plugin-visualizer",
    "start": "vite preview --port 3000",
    "prebuild": "npm run typecheck",
    "postinstall": "husky install",
    "prepare": "husky install",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "nyc report --reporter=text --reporter=html --reporter=lcov",
    "test:coverage:check": "nyc check-coverage --statements 85 --branches 80 --functions 90 --lines 85",
    "test:coverage:full": "nyc npm test && npm run test:coverage:report && npm run test:coverage:check",
    "test:performance": "tsc -p src/tests/tsconfig.json && wtr --config web-test-runner.config.js",
    "test:watch": "wtr --config web-test-runner.config.js --watch",
    "test:lighthouse": "lhci autorun",
    "test:lighthouse:mobile": "lhci autorun --collect.settings.preset=mobile",
    "test:lighthouse:desktop": "lhci autorun --collect.settings.preset=desktop",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:health && npm run test && npm run test:performance && npm run test:lighthouse",
    "test:health": "vitest run src/tests/health/health.test.ts",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --collect.settings.preset=mobile",
    "lighthouse:desktop": "lhci autorun --collect.settings.preset=desktop",
    "build:test": "tsc -p tsconfig.test.json",
    "organize-projects": "node scripts/organizeProjects.mjs && node scripts/generateProjectData.mjs",
    "test:security": "cross-env ZAP_API_URL=http://localhost:8080 playwright test src/tests/security/security.test.ts",
    "test:security:auth": "cross-env ZAP_API_URL=http://localhost:8080 playwright test src/tests/security/security.test.ts -g 'Authentication Security'",
    "test:security:api": "cross-env ZAP_API_URL=http://localhost:8080 playwright test src/tests/security/security.test.ts -g 'API Security'",
    "test:security:headers": "cross-env ZAP_API_URL=http://localhost:8080 playwright test src/tests/security/security.test.ts -g 'Security Headers'",
    "test:security:report": "node src/tests/security/generate-report.js",
    "size": "size-limit",
    "test:ci": "vitest run --coverage",
    "test:e2e:ci": "playwright test",
    "test:smoke": "playwright test src/tests/smoke/smoke.test.ts",
    "test:load": "k6 run load-tests/performance.js",
    "deploy:staging": "wrangler publish --env staging",
    "deploy:prod": "wrangler publish --env production",
    "rollback:staging": "wrangler rollback --env staging",
    "rollback:prod": "wrangler rollback --env production",
    "test:visual": "playwright test --config=playwright.config.ts",
    "test:visual:update": "playwright test --config=playwright.config.ts --update-snapshots",
    "test:environment": "vitest run test/environment/**/*.test.{js,ts} --config vitest.config.environment.ts",
    "test:jsdom": "vitest run test/environment/stability/jsdom/**/*.test.{js,ts} --config vitest.config.environment.ts",
    "test:report": "node scripts/generate-test-report.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "@types/react-helmet-async": "^1.0.1",
    "@types/react-pdf": "^6.2.0",
    "axios": "^1.6.3",
    "framer-motion": "^10.16.16",
    "gsap": "^3.12.5",
    "@tensorflow/tfjs": "^4.15.0",
    "@huggingface/inference": "^2.6.4",
    "pdf-thumbnail": "^1.0.6",
    "pdfjs-dist": "3.4.120",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^9.5.3",
    "react-pdf": "6.2.2",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.10.3",
    "swiper": "^11.0.5",
    "tailwind-merge": "^2.5.4",
    "three": "^0.159.0",
    "three-mesh-bvh": "^0.8.0",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@juggle/resize-observer": "^3.4.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.40.0",
    "@size-limit/preset-app": "^7.0.8",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.10.5",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/sharp": "^0.31.1",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@web/dev-server-esbuild": "^1.0.3",
    "@web/test-runner": "^0.18.0",
    "@web/test-runner-commands": "^0.9.0",
    "@web/test-runner-playwright": "^0.11.0",
    "autoprefixer": "^10.4.16",
    "chrome-launcher": "^1.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.2.0",
    "glob": "^10.3.10",
    "husky": "^9.1.6",
    "jest-snapshot": "^29.7.0",
    "jsdom": "^24.0.0",
    "lighthouse": "^12.2.2",
    "lint-staged": "^15.2.10",
    "lru-cache": "^10.1.0",
    "nyc": "^15.1.0",
    "openai": "^4.28.0",
    "postcss": "^8.4.32",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.33.5",
    "size-limit": "^7.0.8",
    "tailwindcss": "^3.4.0",
    "terser": "^5.36.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-compression2": "^1.3.1",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^1.2.1",
    "web-vitals": "^3.5.0"
  },
  "keywords": [
    "portfolio",
    "react",
    "typescript",
    "vite",
    "tailwindcss",
    "framer-motion",
    "three.js",
    "responsive",
    "dark-mode",
    "animations"
  ],
  "author": "Ralph Bernard Serrano",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
